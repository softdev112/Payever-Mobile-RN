{
  "name": "PayeverMobile",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "npm run env:dev && node node_modules/react-native/local-cli/cli.js start",
    "start:stage": "npm run env:stage && node node_modules/react-native/local-cli/cli.js start",
    "env:dev": "cp src/config/dev.js src/config/_autogenerated.js",
    "env:stage": "cp src/config/stage.js src/config/_autogenerated.js",
    "bump": "gulp bump",
    "reset": "watchman watch-del-all",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "beta": "npm run bump; fastlane beta dev:true",
    "lint": "eslint src",
    "install": "gulp install:git-hooks"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "mobx": "^2.6.5",
    "mobx-logger": "^0.3.1",
    "mobx-react": "^4.0.3",
    "react": "^15.4.1",
    "react-native": "^0.39.2",
    "react-native-extended-stylesheet": "^0.3.0",
    "react-native-fabric": "^0.3.2",
    "react-native-md-textinput": "^2.0.4",
    "react-native-navigation": "^2.0.0-experimental.157"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "17.0.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "cheerio": "^0.22.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "git-validate": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.5.4",
    "gulp-svg2ttf": "^2.0.0",
    "gulp-svgicons2svgfont": "^3.0.1",
    "gulp-task-doc": "^1.0.11",
    "jest": "17.0.0",
    "jest-react-native": "17.0.0",
    "minimist": "^1.2.0",
    "react-test-renderer": "15.3.2",
    "vinyl": "^2.0.1"
  },
  "pre-commit": [
    "lint"
  ]
}
